  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon16.png",
    "128": "images/icon16.png"
  },
  "action": {},
  "permissions": ["activeTab", "storage", "scripting"],
  "content_scripts": [
    {
      "js": ["main.js"],
      "matches": [
        "https://tech.unblockedgames.world/*",
        "https://thewizitale.com/*",
        "https://caronwhaley.com/*"
      ]
    }
  ]
}
